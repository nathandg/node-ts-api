{
   "name": "ts-api",
   "version": "1.0.0",
   "description": "Typescript node.js api",
   "main": "index.js",
   "scripts": {
      "build": "tsc",
      "start": "yarn build && node dist/src/index.js",
      "start:dev": "ts-node-dev './src/index.ts'",
      "lint": "eslint ./src ./test --ext .ts",
      "lint:fix": "eslint ./src ./test --ext .ts --fix",
      "style:check": "prettier --check ./src/**/*.ts ./test/**/*.ts",
      "style:fix": "prettier --write ./src/**/*.ts ./test/**/*.ts",
      "test": "echo \"Error: no test specified\" && exit 1",
      "test:functional": "jest --projects ./test --runInBand",
      "test:unit": "jest"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/nathandg/node-ts-api.git"
   },
   "keywords": [
      "nodejs",
      "typescript"
   ],
   "author": "nathandg",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/nathandg/node-ts-api/issues"
   },
   "homepage": "https://github.com/nathandg/node-ts-api#readme",
   "devDependencies": {
      "@types/axios": "^0.14.0",
      "@types/express": "^4.17.17",
      "@types/jest": "^29.4.0",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^18.11.18",
      "@types/supertest": "^2.0.12",
      "@typescript-eslint/eslint-plugin": "^5.50.0",
      "@typescript-eslint/parser": "^5.50.0",
      "eslint": "^8.33.0",
      "jest": "^29.4.1",
      "prettier": "^2.8.3",
      "supertest": "^6.3.3",
      "ts-jest": "^29.0.5",
      "ts-node-dev": "^2.0.0",
      "typescript": "^4.9.5"
   },
   "dependencies": {
      "@overnightjs/core": "^1.7.6",
      "axios": "^1.3.2",
      "body-parser": "^1.20.1",
      "express": "^4.18.2",
      "module-alias": "^2.2.2"
   }
}
